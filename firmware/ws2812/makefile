.PHONY: all
all: test-ws2812-12f1840


#############################################################################
.PHONY: test-ws2812-12f1840
test-ws2812-12f1840: test-ws2812-12f1840.hex

test-ws2812-12f1840.hex: test-ws2812-12f1840.o
	sdcc --use-non-free -mpic14 -p12f1840 -o $@ $^


#############################################################################
.c.o:
	sdcc --use-non-free -mpic14 -p12f1840 -c $<


#############################################################################
clean :
	$(RM) *.hex
	$(RM) *.lst
	$(RM) *.map
	$(RM) *.cod
	$(RM) *.o
	$(RM) *.asm
	
